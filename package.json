{
  "name": "extensync",
  "version": "1.0.0",
  "description": "Full-stack application with MySQL, React, Express, and Bootstrap",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "setup-db": "cd database && node setup.js"
  },
  "keywords": [
    "mysql",
    "react",
    "express",
    "nodejs",
    "bootstrap"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "dotenv": "^17.2.1",
    "mysql2": "^3.14.3"
  }
}
